else if (category === '') {\\n        name = 'lookbook'\\n        this.routeTo(name, param)\\n      } else if (category === '') {\\n        name = 'paper'\\n        this.routeTo(name, param)\\n      } else if (category === '--') {\\n        name = 'pay'\\n        this.routeTo(name, param)\\n      } else if (category === '-') {\\n        name = 'about'\\n        this.routeTo(name, param)\\n      } else if (category === '') {\\n        name = 'contacts'\\n        this.routeTo(name, param)\\n      }\\n    },\\n    routeTo (name, param) {\\n      this.$router.push({ name: name, params: { name: param } })\\n      this.closeNav()\\n      this.clickedId = ''\\n      this.clicked = 0\\n    },\\n    toggleDropdown (elem) {\\n      if (elem.name.toLowerCase() === '  ') {\\n        this.closeNav()\\n        // var item = document.querySelector('.nav__list-link[data-target=\\\"#js-order\\\"]')\\n        // item.setAttribute('data-toggle', 'modal')\\n      } else {\\n        this.clicked = this.clickedId === elem.id ? 1 : 0\\n        this.clickedId = elem.id\\n        this.showDropdownById = this.showDropdownById === elem.id ? '' : elem.id\\n        if (elem.childs === undefined) {\\n          this.navigateToPage(elem)\\n        } else if (elem.childs !== undefined && this.clicked) {\\n          this.navigateToPage(elem)\\n        }\\n      }\\n\\n    },\\n    hide (e) {\\n      var nav = document.querySelector('#js-nav')\\n      if (!nav.contains(e.target)) {\\n        this.closeNav()\\n        this.showDropdownById = ''\\n      }\\n    }\\n  },\\n  mixins: [hideMixin, hoverMershToggleDelay]\\n}\\n</script>\\n\\n<style lang=\\\"sass\\\">\\n@import '@/sass/_fonts.sass'\\n@import '@/sass/_variables.sass'\\n.nav\\n  z-index: 100\\n  position: absolute\\n  top: 0\\n  left: 0\\n  display: flex\\n  flex-wrap: wrap\\n  justify-content: flex-end\\n  // flex-direction: column\\n  // width: 725px\\n  width: 37.760vw\\n  height: 100vh\\n  // overflow-y: scroll\\n  background-color: $color