  'nav',\\n      'showNav'\\n    ])\\n  },\\n  methods: {\\n    ...mapActions({\\n      'hideNav': 'hideNav',\\n      'switchShowNav': 'switchShowNav'\\n    }),\\n    toggleNav () {\\n      var app = document.getElementById('app')\\n      app.classList.toggle('fixed')\\n      this.switchShowNav()\\n    },\\n    closeNav () {\\n      var app = document.getElementById('app')\\n      app.classList.remove('fixed')\\n      this.clickedId = ''\\n      this.hideNav()\\n    },\\n    navigateToPage (elem) {\\n      this.$router.push({name: elem.link, params: { name: elem.name }})\\n      this.closeNav()\\n      this.clickedId = ''\\n      this.clicked = false\\n    },\\n    toggleDropdown (elem) {\\n      this.clicked = this.clickedId === elem.id ? true : false\\n      this.clickedId = elem.id\\n      this.showDropdownById = this.showDropdownById === elem.id ? '' : elem.id\\n      if (elem.children === undefined) {\\n        this.navigateToPage(elem)\\n      } else if (elem.children !== undefined && this.clicked) {\\n        this.navigateToPage(elem)\\n      }\\n    },\\n    hide (e) {\\n      var nav = document.querySelector('#js-nav')\\n      if (!nav.contains(e.target)) {\\n        this.closeNav()\\n        this.showDropdownById = ''\\n      }\\n    }\\n  },\\n  mixins: [hideMixin, hoverMershToggleDelay]\\n}\\n</script>\\n\\n<style lang=\\\"sass\\\">\\n@import '@/sass/_fonts.sass'\\n@import '@/sass/_variables.sass'\\n.nav\\n  z-index: 10\\n  position: absolute\\n  top: 0\\n  left: 0\\n  display: flex\\n  flex-wrap: wrap\\n  justify-content: flex-end\\n  // flex-direction: column\\n  // width: 725px\\n  width: 37.760vw\\n  height: 100vh\\n  // overflow-y: scroll\\n  background-color: $color-nav\\n\\n.nav__header\\n  width: 100%\\n  height: $height-header\\n  // padding-left: 128px\\n  padding-left: 6.667vw\\n\\n.nav__close-wrapper\\n  width: auto\\n  height: 2.760vw\\n\\n.nav__close\\n  padding: 0\\n  background-color: transparent\\n  border: none\\n  transition: opacity 0