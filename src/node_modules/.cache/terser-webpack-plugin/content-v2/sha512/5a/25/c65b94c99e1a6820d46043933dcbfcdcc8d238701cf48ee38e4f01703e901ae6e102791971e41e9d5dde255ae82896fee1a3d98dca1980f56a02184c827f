_checkbox p-icon\\\" v-model=\\\"checkboxPrivacy\\\">\\n            <i class=\\\"icon fas fa-check d-flex align-items-center justify-content-center\\\" slot=\\\"extra\\\"></i>\\n            <p class=\\\"order-register__checkbox-descr d-flex flex-wrap mb-0\\\">\\n              &nbsp;\\n            <router-link class=\\\"order-register__link\\\"\\n              tag=\\\"a\\\"\\n              :to=\\\"{ name: 'privacy' }\\\">\\n               \\n            </router-link>\\n                  \\n            </p>\\n          </p-check>\\n\\n          </div>\\n\\n          <vue-recaptcha\\n            class=\\\"recaptcha\\\"\\n            ref=\\\"recaptcha\\\"\\n            :sitekey=\\\"sitekey\\\"\\n            @verify=\\\"register\\\"\\n            @expired=\\\"onCaptchaExpired\\\"\\n          />\\n\\n          <div class=\\\"order-register__buttons d-flex flex-column flex-sm-row align-items-center justify-content-between\\\">\\n            <router-link class=\\\"order-register__cancel button button_form button_form-page order-2 order-sm-1\\\"\\n              :to=\\\"{ name: 'cart' }\\\"\\n              tag=\\\"a\\\">\\n                \\n            </router-link>\\n\\n            <button class=\\\"order-register__submit button button_form button_form-page button_submit order-1 order-sm-2\\\"\\n              @click.prevent=\\\"submit\\\"\\n              :disabled=\\\"$v.$invalid || !recaptcha || !checkboxPrivacy\\\">\\n              <span v-if=\\\"message !== ''\\\" class=\\\"button_submit-message\\\">{{ message }}</span>\\n              <span v-else class=\\\"button_submit-text\\\"></span>\\n            </button>\\n          </div>\\n\\n        </form>\\n      </div>\\n    </div>\\n  </div>\\n</template>\\n\\n<script>\\nimport VueRecaptcha from 'vue-recaptcha'\\nimport { required, minLength, email, requiredUnless } from 'vuelidate/lib/validators'\\nimport AwesomeMask from 'awesome-mask'\\nimport axios from 'axios'\\nimport { mapGetters, mapActions } from 'vuex'\\nimport