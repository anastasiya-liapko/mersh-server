ick.$slides[nextSlide].querySelector('video').currentTime = 0;\n        slick.$slides[nextSlide].querySelector('video').play();\n      }\n    },\n    handleInit: function handleInit(event, slick) {\n      this.$refs.slick.reSlick();\n      var video = document.querySelector('.slick-current video');\n      video ? video.play() : '';\n    }\n  },\n  watch: {\n    showMute: function showMute() {\n      var video = document.querySelector('.slick-active video');\n\n      if (video) {\n        this.showMute ? video.muted = true : video.muted = false;\n      }\n    },\n    options: function options() {\n      this.$refs.slick.reSlick();\n    }\n  },\n  components: {\n    'app-slick': Slick\n  }\n};",{"version":3,"sources":["PopupPreview.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2DA,OAAA,KAAA,MAAA,WAAA;AAEA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,EAAA,EAAA,YADA;AAEA,MAAA,QAAA,EAAA;AAFA,KAAA;AAIA,GANA;AAOA,EAAA,KAAA,EAAA,CAAA,SAAA,EAAA,SAAA,CAPA;AAQA,EAAA,OARA,qBAQA;AACA,QAAA,IAAA,GAAA,IAAA;AACA,IAAA,CAAA,CAAA,MAAA,KAAA,EAAA,CAAA,CAAA,EAAA,CAAA,gBAAA,EAAA,YAAA;AACA,MAAA,IAAA,CAAA,KAAA,CAAA,KAAA,CAAA,OAAA;AACA,KAFA;AAGA,GAbA;AAcA,EAAA,OAAA,EAAA;AACA,IAAA,kBADA,8BACA,KADA,EACA,KADA,EACA,YADA,EACA,SADA,EACA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,YAAA,EAAA,aAAA,CAAA,OAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,YAAA,EAAA,aAAA,CAAA,OAAA,EAAA,KAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,YAAA,EAAA,aAAA,CAAA,OAAA,EAAA,KAAA;AACA,aAAA,QAAA,GAAA,IAAA;AACA;;AAEA,UAAA,KAAA,CAAA,OAAA,CAAA,SAAA,EAAA,aAAA,CAAA,OAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,SAAA,EAAA,aAAA,CAAA,OAAA,EAAA,WAAA,GAAA,CAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,SAAA,EAAA,aAAA,CAAA,OAAA,EAAA,IAAA;AACA;AACA,KAZA;AAaA,IAAA,UAbA,sBAaA,KAbA,EAaA,KAbA,EAaA;AACA,WAAA,KAAA,CAAA,KAAA,CAAA,OAAA;AACA,UAAA,KAAA,GAAA,QAAA,CAAA,aAAA,CAAA,sBAAA,CAAA;AACA,MAAA,KAAA,GAAA,KAAA,CAAA,IAAA,EAAA,GAAA,EAAA;AACA;AAjBA,GAdA;AAiCA,EAAA,KAAA,EAAA;AACA,IAAA,QAAA,EAAA,oBAAA;AACA,UAAA,KAAA,GAAA,QAAA,CAAA,aAAA,C