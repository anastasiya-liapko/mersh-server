"order-register__field d-flex flex-wrap align-items-center justify-content-between\">\n            <label class=\"order-register__label mb-0\">Имя</label>\n            <input class=\"order-register__input\"\n              :class=\"{invalid: $v.data.name.$error}\"\n              type=\"text\"\n              name=\"name\"\n              placeholder=\"Иван Петров\"\n              @blur=\"$v.data.name.$touch()\"\n              v-model=\"data.name\">\n            <p class=\"order-register__error-text\"\n              v-if=\"$v.data.name.$error\">Укажите имя</p>\n          </div>\n\n          <div class=\"order-register__field d-flex flex-wrap align-items-center justify-content-between\">\n            <label class=\"order-register__label mb-0\">Телефон</label>\n            <input class=\"order-register__input\"\n              :class=\"{invalid: $v.data.phone.$error}\"\n              type=\"text\"\n              name=\"phone\"\n              placeholder=\"+ 7 (ххх) ххх хх хх\"\n              v-mask=\"'+9 (999) 999-99-99'\"\n              @blur=\"$v.data.phone.$touch()\"\n              v-model=\"data.phone\">\n            <transition name=\"fade\">\n              <p class=\"order-register__error-text\"\n                v-if=\"!$v.data.phone.required && $v.data.phone.$dirty\">\n                Укажите телефон или email\n              </p>\n              <p class=\"order-register__error-text\"\n                v-if=\"!$v.data.phone.minLength\">\n                Введите телефон в формате + 7 (ххх) ххх хх хх\n              </p>\n            </transition>\n          </div>\n\n          <div class=\"order-register__field d-flex flex-wrap align-items-center justify-content-between\">\n            <label class=\"order-register__label mb-0\">Email</label>\n            <input class=\"order-register__input\"\n              :class=\"{invalid: $v.data.email.$error}\"\n              type=\"text\"\n              name=\"email\"\n              placeholder=\"mersh@site.com\"\n              @blur=\"$v.data.email.$touch()\"\n              v-model=\"data.email\">\n            <transition name=\"fade\">\n              <p class=\"order-register__error-text\"\n                v-if=\"!$v.data.email.required && $v.data.email.$dirty\">\n                Укажите телефон или email\n              </p>\n              <p class=\"order-register__error-text\"\n                v-if=\"!$v.data.email.email\">\n                Введите email в формате mersh@site.com\n              </p>\n            </transition>\n          </div>\n\n          <div class=\"order-register__select-wrapper select-wrapper d-flex align-items-center justify-content-between\"\n            :class=\"{invalid: !$v.data.delivery.required && $v.data.delivery.$invalid && touchDelivery}\"\n            style=\"border: 1px solid red\">\n            <label class=\"order-register__label mb-0\">Способ доставки</label>\n            <app-select\n              :id=\"'js-selectDelivery'\"\n              class=\"order-register__select select select_type_register\"\n              :show=\"selectShow && selectName === 'delivery'\"\n              :name=\"'delivery'\"\n              :placeholder=\"'Курьер'\"\n              :options=\"selectDelivery\"\n              :selected=\"data.delivery\"\n              v-on:updateOption=\"select\">\n            </app-select>\n            <transition name=\"fade\">\n              <p class=\"order-register__error-text order-register__error-text_type_select\"\n                v-if=\"!$v.data.delivery.required && $v.data.delivery.$invalid && touchDelivery\">\n                Укажите способ доставки\n              </p>\n            </transition>\n          </div>\n\n          <div class=\"order-register__field d-flex flex-wrap align-items-center justify-content-between\">\n            <label class=\"order-register__label mb-0\">Адрес</label>\n            <input class=\"order-register__input\"\n              :class=\"{invalid: $v.data.address.$error}\"\n              type=\"text\"\n              name=\"address\"\n              placeholder=\"Улица\"\n              @blur=\"$v.data.address.$touch()\"\n              v-model=\"data.address\">\n            <p class=\"order-register__error-text\"\n              v-if=\"$v.data.address.$error\">Укажите адрес</p>\n          </div>\n\n          <div class=\"order-register__select-wrapper select-wrapper d-flex align-items-center justify-content-between\"\n            :class=\"{invalid: !$v.data.payment.required && $v.data.payment.$invalid && touchPayment}\"\n            style=\"border: 1px solid red\">\n            <label class=\"order-register__label mb-0\">Способ оплаты</label>\n            <app-select\n              :id=\"'js-selectPayment'\"\n              class=\"order-register__select select select_type_register\"\n              :show=\"selectShow && selectName === 'payment'\"\n              :name=\"'payment'\"\n              :placeholder=\"'Наличные'\"\n              :options=\"selectPayment\"\n              :selected=\"data.payment\"\n              v-on:updateOption=\"select\">\n            </app-select>\n            <transition name=\"fade\">\n              <p class=\"order-register__error-text order-register__error-text_type_select\"\n                v-if=\"!$v.data.payment.required && $v.data.payment.$invalid && touchPayment\">\n                Укажите cпособ оплаты\n              </p>\n            </transition>\n          </div>\n\n          <div class=\"order-register__field d-flex flex-wrap align-items-center justify-content-between\">\n            <label class=\"order-register__label mb-0\">Сообщение</label>\n            <textarea class=\"order-register__textarea\"\n              type=\"text\"\n              name=\"message\"\n              placeholder=\"Текст\"\n              @blur=\"$v.data.message.$touch()\"\n              v-model=\"data.message\"></textarea>\n          </div>\n          </div>\n\n          <vue-recaptcha\n            class=\"recaptcha\"\n            ref=\"recaptcha\"\n            :sitekey=\"sitekey\"\n            @verify=\"register\"\n            @expired=\"onCaptchaExpired\"\n          />\n\n          <div class=\"order-register__buttons d-flex align-items-center justify-content-between\">\n            <router-link class=\"order-register__cancel button button_form\"\n              :to=\"{ name: 'cart' }\"\n              tag=\"a\">\n                Отмена\n            </router-link>\n\n            <button\n              class=\"order-register__submit button button_form button_submit\"\n              @click.prevent=\"submit\"\n              :disabled=\"$v.$invalid || !recaptcha\">\n              <span v-if=\"message !== ''\" class=\"button_submit-message\">{{ message }}</span>\n              <span v-else class=\"button_submit-text\">Отправить</span>\n            </button>\n          </div>\n\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport VueRecaptcha from 'vue-recaptcha'\nimport { required, minLength, email, requiredUnless } from 'vuelidate/lib/validators'\nimport AwesomeMask from 'awesome-mask'\nimport axios from 'axios'\nimport { mapGetters, mapActions } from 'vuex'\nimport Select from '@/components/Select.vue'\n\nexport default {\n  name: 'order-register',\n  data () {\n    return {\n      id: 'js-orderRegister',\n      data: {\n        name: '',\n        phone: '',\n        email: '',\n        delivery: '',\n        address: '',\n        payment: '',\n        message: '',\n        recaptchaToken: ''\n      },\n      message: '',\n      sitekey: process.env.VUE_APP_RECAPTCHA_TOKEN,\n      recaptcha: false,\n      touchDelivery: false,\n      touchPayment: false\n    }\n  },\n  computed: {\n    ...mapGetters([\n      'selectDelivery',\n      'selectPayment',\n      'selectName',\n      'selectShow'\n    ])\n  },\n  validations: {\n    data: {\n      name: {\n        required\n      },\n      phone: {\n        required,\n        minLength: minLength(18)\n      },\n      email: {\n        email\n      },\n      delivery: {\n        required\n      },\n      address: {\n        required\n      },\n      payment: {\n        required\n      }\n    }\n  },\n  methods: {\n    ...mapActions([\n      'filterSelect',\n      'resetSelect'\n    ]),\n    select(value) {\n      switch(value[0]) {\n        case 'delivery':\n          this.data.delivery = value[1].name\n          this.touchDelivery = true\n          break\n        case 'payment':\n          console.log('value1 ' + value[1].id)\n          this.data.payment = value[1].name\n          this.touchPayment = true\n          break\n      }\n    },\n    resetForm () {\n      var self = this\n      Object.keys(self.data).forEach(function (key, index) {\n        self.data[key] = ''\n      })\n      self.$v.$reset()\n      this.touchDelivery = false\n      this.touchPayment = false\n    },\n    showMessage (text) {\n      var self = this\n      this.message = text\n      setTimeout(function () {\n        self.message = ''\n      }, 1500)\n    },\n    register (recaptchaToken) {\n      this.recaptcha = true\n      this.data.recaptchaToken = recaptchaToken\n    },\n    submit () {\n      var self = this\n      console.log(self.data)\n      axios.post('/post.php', self.data)\n        .then(function (response) {\n          console.log(response)\n          self.showMessage('Отправлено!')\n          self.resetForm()\n          self.$refs.recaptcha.reset()\n          self.recaptcha = false\n          this.$router.push({ name: 'order-accept' })\n        })\n        .catch(function (error) {\n          console.log(error)\n          self.showMessage('Ошибка!')\n        })\n    },\n    onCaptchaExpired () {\n      this.$refs.recaptcha.reset()\n      this.recaptcha = false\n    }\n  },\n  directives: {\n    'mask': AwesomeMask\n  },\n  components: {\n    'vue-recaptcha': VueRecaptcha,\n    'app-select': Select\n  }\n}\n</script>\n\n<style lang=\"sass\">\n@import '@/sass/_variables.sass'\n.order-register\n  .container-main\n    bor