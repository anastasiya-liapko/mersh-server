s=\"modal-content\">\n        <button type=\"button\" class=\"close d-flex align-items-center justify-content-center\" data-dismiss=\"modal\" aria-label=\"Close\">\n          <span class=\"icon-close d-flex align-items-center justify-content-center\" aria-hidden=\"true\"></span>\n        </button>\n        <form class=\"popup-form d-flex flex-column align-items-center justify-content-center\">\n          <h2 class=\"popup-form__title\">Заказать звонок</h2>\n\n          <div class=\"popup-form__field d-flex flex-wrap align-items-center justify-content-between\">\n            <label class=\"popup-form__label mb-0\">Имя</label>\n            <input class=\"popup-form__input\"\n              :class=\"{invalid: $v.data.name.$error}\"\n              type=\"text\"\n              name=\"name\"\n              placeholder=\"Иван Петров\"\n              @blur=\"$v.data.name.$touch()\"\n              v-model=\"data.name\">\n            <p class=\"popup-form__error-text\"\n              v-if=\"$v.data.name.$error\">Укажите имя</p>\n          </div>\n\n          <div class=\"popup-form__field d-flex flex-wrap align-items-center justify-content-between\">\n            <label class=\"popup-form__label mb-0\">Телефон</label>\n            <input class=\"popup-form__input\"\n              :class=\"{invalid: $v.data.phone.$error}\"\n              type=\"text\"\n              name=\"phone\"\n              placeholder=\"+ 7 (ххх) ххх хх хх\"\n              v-mask=\"'+9 (999) 999-99-99'\"\n              @blur=\"$v.data.phone.$touch()\"\n              v-model=\"data.phone\">\n            <transition name=\"fade\">\n            <p class=\"popup-form__error-text\"\n              v-if=\"$v.data.phone.$error\">Укажите телефон</p>\n            </transition>\n          </div>\n\n          <button\n              class=\"popup-form__submit button button_form\"\n              @click.prevent=\"submit\"\n              :disabled=\"$v.$invalid\">\n              Отправить\n          </button>\n\n          <!-- <p  class=\"popup-form__message text-center\"\n              v-if=\"send\">\n              {{ send }}\n          </p> -->\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { required, minLength } from 'vuelidate/lib/validators'\nimport AwesomeMask from 'awesome-mask'\nimport axios from 'axios'\n\nexport default {\n  data () {\n    return {\n      id: 'js-call',\n      data: {\n        name: '',\n        phone: ''\n      },\n      send: ''\n    }\n  },\n  mounted () {\n    var self = this\n    $('#' + this.id).on('hidden.bs.modal', function () {\n      Object.keys(self.data).forEach(function (key, index) {\n        self.data[key] = ''\n      })\n      self.$v.$reset()\n    })\n    // $('#' + this.id).on('show.bs.modal', function () {\n    //   setTimeout(function () {\n    //     document.querySelector('.popup input').focus()\n    //   }, 400)\n    // })\n  },\n  validations: {\n    data: {\n      name: {\n        required\n      },\n      phone: {\n        required,\n        minLength: minLength(18)\n      }\n    }\n  },\n  methods: {\n    submit () {\n      var self = this\n      axios.post('/post.php', this.data)\n        .then(function (response) {\n          console.log(response)\n          self.send = 'Успешно!'\n          // self.resetForm()\n        })\n        .catch(function (error) {\n          console.log(error)\n          self.send = 'Ошибка!'\n        })\n    }\n  },\n  directives: {\n    'mask': AwesomeMask\n  }\n}\n</script>\n\n<style lang=\"sass\">\n@import '@/sass/_variables.sass'\n.popup\n  .modal-dialog\n    max-width: 735px\n    width: 735px\n    height: auto\n  .modal-content\n    padding: 68px\n    border: none\n    border-radius: 0\n  .close\n    position: absolute\n    top: 0\n    right: 0\n    width: 68px\n    height: 68px\n    background-color: #e4e4e4\n    opacity: 1\n    transition: opacity 0.3s\n    &:hover\n      opacity: 0.7\n    .icon-close\n      &::before\n        font-size: 17px\n\n.popup-form\n  width: 100%\n\n.popup-form__title\n  margin-bottom: 5px\n  font-family: $font-cormorant-sc\n  font-size: 24px\n  font-weight: 700\n  line-height: 1\n  color: $color-black\n\n.popup-form__field\n  position: relative\n  width: 100%\n  margin-top: 25px\n\n.popup-form__label,\n.popup-form__input,\n.popup-form__input::placeholder\n  font-family: $font-montserrat\n  font-size: 18px\n  font-weight: 400\n  line-height: 1\n  color: $color-black\n\n.popup-form__input\n  width: 435px\n  padding: 25px 0\n  border: none\n  border-bottom: 1px solid #979797\n  transition: border 0.3s linear\n  &.invalid\n    border-bottom: 1px solid red\n\n.popup-form__error-text\n  position: absolute\n  top: 100%\n  right: 0\n  width: 435px\n  margin-top: 5px\n  margin-bottom: 0\n  font-family: $font-montserrat\n  font-size: 18px\n  font-weight: 400\n  line-height: 1\n  color: red\n\n.fade-enter,\n.fade-enter-active\n  animation: fade-in 0.3s cubic-bezier(0.390, 0.575, 0.565, 1.000) both\n\n.fade-leave,\n.fade-leave-active\n  animation: fade-out 0.25s ease-out both\n\n@keyframes fade-in\n  0%\n    opacity: 0\n  100%\n    opacity: 1\n\n@keyframes fade-out\n  0%\n    opacity: 1\n  100%\n    opacity: 0\n\n.popup-form__submit\n  margin-top: 25px\n\n.call\n  .popup-form__submit\n    margin-top: 65px\n</style>\n"]}]}