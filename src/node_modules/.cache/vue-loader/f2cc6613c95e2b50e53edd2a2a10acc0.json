nts","sourcesContent":["<template>\n  <div class=\"cart__form-product d-flex flex-wrap flex-sm-nowrap justify-content-center align-items-center w-100\">\n    <div class=\"cart__form-name order-1 order-sm-1\">{{ product.name }}</div>\n    <div class=\"cart__form-variants order-3 order-sm-2\">\n      <div v-if=\"product.variants\" class=\"cart__select-wrapper select-wrapper d-flex flex-wrap align-items-center justify-content-between\">\n        <app-select\n          :id=\"'js-selectCart-' + product.id\"\n          class=\"cart__select select select_type_cart\"\n          :show=\"selectShow && selectName === 'cart-' + product.id\"\n          :name=\"'cart-' + product.id\"\n          :placeholder=\"'Вариант 1'\"\n          :options=\"product.variants\"\n          :selected=\"product.variant\"\n          v-on:updateOption=\"select\">\n        </app-select>\n      </div>\n    </div>\n\n    <div class=\"cart__form-price order-4 order-sm-3\">{{ product.price }} руб.</div>\n    <div class=\"cart__form-multiplication order-5 order-sm-4 d-flex align-items-center justify-content-center\">\n      <span class=\"icon\">x</span>\n    </div>\n    <div class=\"cart__form-quantity order-6 order-sm-5\">\n      <input class=\"cart__form-input\" type=\"text\" placeholder=\"12\" v-model=\"quantity\" :val=\"getQuantity\">\n    </div>\n    <div class=\"cart__form-total order-7 order-sm-6\">15 000 руб.</div>\n    <div class=\"cart__form-del order-2 order-sm-7 d-flex align-items-center justify-content-center\"\n      @click=\"removeFromCart(product.id)\">\n      <span class=\"icon icon-delete d-flex align-items-center justify-content-center\"></span>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Select from '@/components/Select.vue'\nimport { mapGetters, mapActions } from 'vuex'\n\nexport default {\n  data () {\n    return {\n      quantity: 0\n    }\n  },\n  props: ['product'],\n  computed: {\n    ...mapGetters([\n      'selectName',\n      'selectShow'\n    ]),\n    getQuantity () {\n      return this.quantity = this.product.quantity\n    }\n  },\n  methods: {\n    ...mapActions([\n      'removeFromCart',\n      'changeVariantInCart'\n    ]),\n    select (value) {\n      var id = value[0].split('-').pop()\n      console.log(value[1].name)\n      this.changeVariantInCart([id, value[1].name])\n      // console.log(this.cartProducts)\n    }\n  },\n  components: {\n    'app-select': Select\n  }\n}\n</script>\n\n<style lang=\"sass\">\n\n</style>\n"]}]}